				float2 temp_cast_3 = ((normal_light_dir51*_LightStrength + _LightStrength)).xx;
				float4 shadow17 = ( lerpResult34 * ( tex2D( _ToonRamp, temp_cast_3 ) * _RampTint ) );
				float4 Lighting13 = ( shadow17 * _MainLightColor );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
