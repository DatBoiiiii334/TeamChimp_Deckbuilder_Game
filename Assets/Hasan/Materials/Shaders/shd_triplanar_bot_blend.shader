				float smoothstepResult53 = smoothstep( _specmin , _specmax , pow( dotResult22 , _Specpower ));
				float4 triplanar41 = TriplanarSampling41( _PaintSrokes, WorldPosition, ase_worldNormal, 1.0, _BrushStrokesTiling, 1.0, 0 );
				float4 triplanar91 = TriplanarSampling91( _BottomBlend, WorldPosition, ase_worldNormal, 1.0, _BottomBlendTiling, 1.0, 0 );
				float2 uv_GradientNoise1 = IN.ase_texcoord4.xy * _GradientNoise1_ST.xy + _GradientNoise1_ST.zw;
				float4 tex2DNode30 = tex2D( _GradientNoise1, uv_GradientNoise1 );
